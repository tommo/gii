QWidget
{
	font-size:11px;
	font-family: Arial;
	color:#cacaca;
	background-color:#333;
}

QFrame
{
	border:0;
}

QLabel{
	background-color:transparent;
}

QToolButton
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#444,stop:1#333);
	border: 1px solid #222;
	border-radius:2px;
	color:#ccc;
	padding:4px;
}

QToolButton::hover
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#444,stop:1#444);
	color:#eee;
}

QPushButton
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#555,stop:1#444);
	border: 1px solid #222;
	border-radius:2px;
	color:#dcdcdc;
	padding:4px;
	margin:1px;
}

QPushButton::hover
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#7b7b7b,stop:1#676767);
	color:#fff;
}

QDialogButtonBox > QPushButton,QDialog > QPushButton,QWizard > QWidget > QPushButton
{
	min-width:5em;
	min-height:1.4em;
}

QCheckBox
{
	margin-bottom: 5px;
	margin-top: 6px;
	background-color:none;
}

QDockWidget
{
	background-color:#464646;
	color:#a5a5a5;
	padding: 2px;
}

QDockWidget::title
{
  background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #444, stop:1 #222);
	padding-top: 5px;
	padding-left: 5px;
	border: 1px solid #222;
}


QHeaderView::section
{
	color:#b9b9b9;
  background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #444, stop:1 #333);
  border-top:0 solid #000;
	border-right:1 solid #000;
	border-bottom:1px solid #222;
	border-left:1px solid #444;
	padding:2px;
	padding-left:5px;
}

QHeaderView::section:first
{
	border-left:0 solid #000;
}

QHeaderView::down-arrow
{
	image:url(theme:arrow_down_V2.png);
	width:10px;
	height:10px;
	padding-right:1ex;
}

QHeaderView::up-arrow
{
	image:url(theme:arrow_up_V2.png);
	width:10px;
	height:10px;
	padding-right:1ex;
}

QScrollBar
{
	background-color: #444;
}

QScrollBar::add-page,QScrollBar::sub-page
{
	background:none;
}

QScrollBar::horizontal
{
	border:0px solid #303030;
	border-left:0 solid red;
	border-right:0 solid red;
	margin:0 11px;
	background-color: #444;
	height: 10px;
}

QScrollBar::vertical
{
	border:0px solid #303030;
	border-top:0 solid red;
	border-bottom:0 solid red;
	margin:11px 0;
	background-color: #444;
	width: 10px;
}

QScrollBar::handle
{
	background-color:#666;
	border: 0px;
}

QScrollBar::handle:hover
{
	background-color:#888;
}

QScrollBar::handle:horizontal
{
	min-width:10px;
}

QScrollBar::handle:vertical
{
	min-height:10px;
}

QScrollBar::handle:pressed
{
	background-color:#828282;
}

QScrollBar::add-line:horizontal
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:1,y2:0,stop:0#3c3c3c,stop:1#5a5a5a);*/
	background-color: #555;
	width:10px;
	subcontrol-position:right;
	subcontrol-origin:margin;
}

QScrollBar::sub-line:horizontal
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:1,y2:0,stop:0#5a5a5a,stop:1#3c3c3c);*/
	background-color: #555;
	width:10px;
	subcontrol-position:left;
	subcontrol-origin:margin;
}

QScrollBar::add-line:horizontal:hover
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:1,y2:0,stop:0#646464,stop:1#969696);*/
	background-color: #888;
}

QScrollBar::sub-line:horizontal:hover
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:1,y2:0,stop:0#969696,stop:1#646464);*/
	background-color: #888;
}

QScrollBar::add-line:horizontal:pressed
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:1,y2:0,stop:0#828282,stop:1#b4b4b4);*/
	background-color: #888;
}

QScrollBar::sub-line:horizontal:pressed
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:1,y2:0,stop:0#b4b4b4,stop:1#828282);*/
	background-color: #888;
}

QScrollBar::add-line:vertical
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#3c3c3c,stop:1#5a5a5a);*/
	background-color: #555;
	height:10px;
	subcontrol-position:bottom;
	subcontrol-origin:margin;
}

QScrollBar::sub-line:vertical
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#5a5a5a,stop:1#3c3c3c);*/
	background-color: #555;
	height:10px;
	subcontrol-position:top;
	subcontrol-origin:margin;
}

QScrollBar::add-line:vertical:hover
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#646464,stop:1#969696);*/
	background-color: #888;
}

QScrollBar::sub-line:vertical:hover
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#969696,stop:1#646464);*/
	background-color: #888;
}

QScrollBar::add-line:vertical:pressed
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#828282,stop:1#b4b4b4);*/
	background-color: #888;
}

QScrollBar::sub-line:vertical:pressed
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#b4b4b4,stop:1#828282);*/
	background-color: #888;
}

QScrollBar::left-arrow:horizontal
{
	background-image/*:url(theme:arrow_left_V2.png);*/
	background-position:center;
}

QScrollBar::right-arrow
{
	/*background-image:url(theme:arrow_right_V2.png);*/
	background-position:center;
}

QScrollBar::up-arrow:vertical
{
	/*background-image:url(theme:arrow_up_V2.png);*/
	background-position:center;
}

QScrollBar::down-arrow:vertical
{
	/*background-image:url(theme:arrow_down_V2.png);*/
	background-position:center;
}

QSplitter::handle
{
	background-color:#282828;
	border:0 solid green;
}

QTreeView, QListView
{
	background-color:#444;
	alternate-background-color:#333;
	border:1px solid #222;
}

QTreeView::item
{
	color:#dcdcdc;
	border:0 solid green;
}

QTreeView::branch:closed:has-children:!has-siblings,QTreeView::branch:closed:has-children:has-siblings
{
	image:url(theme:treeview_arrow_right.png);
}

QTreeView::branch:open:has-children:!has-siblings,QTreeView::branch:open:has-children:has-siblings
{
	image:url(theme:treeview_arrow_down.png);
}

QTreeView::indicator
{
	border:0 solid red;
	width:14px;
	height:16px;
}

QTreeView#UIGroup_TreeView
{
	background-color:#282828;
	alternate-background-color:#323232;
	border:0 solid #000;
}

QListView::item:selected
{
	color:#fff;
}

QListView#UIGroup_ListView
{
	background-color:#282828;
	border:0 solid #000;
	selection-background-color:#3c3c3c;
}

QMenuBar::item
{
	background-color:transparent;
	color:#dcdcdc;
}

QMenu
{
	background-color:#222222;
	color:#dcdcdc;
	border:0 solid red;
	padding:5px;
}

QMenu::item
{
	background-color:transparent;
	border:1px solid transparent;
	padding:2px 25px 2px 20px;
}

QMenu::item:selected
{
	background-color:#466dc6;
	border:1px solid transparent;
	color:#dcdcdc;
}

QMenu::separator
{
	height:1px;
	background-color:#464646;
	margin:2px 25px 2px 20px;
}

QDialog
{
	background-color:#222;
	border:0 solid #000;
}

QTabWidget > QWidget >QWidget {
	background-color:#333;
}

QTabWidget::pane
{
	border:1px solid #222;
	/*margin-top:-1px;*/
}

QTabWidget::tab-bar
{
	background-color:#000;
	left:0px;
	bottom:0px;
	border:1px solid #000;
}

QTabBar::tab
{
  background-color: QLinearGradient(x1:0, y1:0, x2:0, y2:1, stop:0 #444, stop:1 #333);
	color:#a1a1a1;
	padding:4px 5px;
	border-top: 1px solid #222;
	border-right: 1px solid #222;
	border-left: 1px solid #444;
}

QTabBar::tab:first{
	border-left: 1px solid #222;
}

QTabBar::tab:selected
{
	background-color:#666;
	color:#c8c8c8;
	padding:4px 5px;
}

QProgressBar
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#5a5a5a,stop:1#3c3c3c);
	border:1px solid #000;
}

QProgressBar::chunk
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0rgba(180,180,0,100), stop:1 rgba(60,60,60,60));
	border:1px solid rgba(180,180,180,200);
}

QToolBar
{
	border:1px solid #222;
	padding:1px;
	spacing:2px;
}

QToolBar::handle
{
	margin:2px;
	border-left: 1px solid #222;
	border-top: 1px solid #222;
	border-right: 1px solid #333;
	border-bottom: 1px solid #333;
	background-color: transparent;	
}

QToolBar:separator
{
	background-color:#464646;
	border-right:0 solid #000;
	border-bottom:0 solid #000;
	width:1px;
	height:1px;
	margin:1px 5px;
}

QSlider::groove:horizontal
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#b4b4b4,stop:1#8c8c8c);
	border:1px solid #3c3c3c;
	height:4px;
}

QSlider::handle:horizontal
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#b4b4b4,stop:1#8c8c8c);
	border:1px solid #3c3c3c;
	width:8px;
	border-radius:2px;
	margin:-2px 0;
}

QSlider::handle:horizontal:hover
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#dcdcdc,stop:1#b4b4b4);
}

QStatusBar
{
	background-color:#282828;
	color:#dcdcdc;
	border:0 solid red;
}

DlgSettings QToolButton
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#5a5a5a,stop:1#3c3c3c);
	border:1px solid #000;
	color:#dcdcdc;
}

DlgSettings QTabWidget::pane
{
	background-color:#3c3c3c;
}

DlgSettings QTabBar[shape="0"]::tab:selected
{
	background-color:#3c3c3c;
	color:#dcdcdc;
	border-top:1px solid #466dc6;
}

