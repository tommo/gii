{
	background-color:transparent;
	border:0 solid red;
	color:#dcdcdc;
	padding: 2px;
}
*/

QToolButton::menu-button
{
	border:0 solid red;
}

QSpinBox,QDoubleSpinBox
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#b4b4b4,stop:1#969696);*/
	border:1px solid #000;
	/*selection-background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#787878,stop:1#5a5a5a);*/
	/*selection-color:#191919;*/
	padding:0;
}

QSpinBox::up-button,QSpinBox::down-button,QDoubleSpinBox::up-button,QDoubleSpinBox::down-button
{
	subcontrol-origin:border;
	width:16px;
	border-width:0;
}

QSpinBox::up-button,QDoubleSpinBox::up-button
{
	subcontrol-position:top right;
}

QSpinBox::down-button,QDoubleSpinBox::down-button
{
	subcontrol-position:bottom right;
}

QSpinBox::up-arrow,QDoubleSpinBox::up-arrow
{
	border-image:url(theme:spinbox_up.png);
	height:7px;
}

QSpinBox::down-arrow,QDoubleSpinBox::down-arrow
{
	border-image:url(theme:spinbox_down.png);
	height:7px;
}

QRadioButton::indicator:unchecked
{
	image:url(theme:radiobutton_unchecked.png);
}

QRadioButton::indicator:unchecked:disabled
{
	image:url(theme:radiobutton_unchecked_disabled.png);
}

QRadioButton::indicator:checked
{
	image:url(theme:radiobutton_checked.png);
}

QRadioButton::indicator:checked:disabled
{
	image:url(theme:radiobutton_checked_disabled.png);
}

QLineEdit, QTextEdit, QPlainTextEdit, QSpinBox, QDoubleSpinBox
{
	background-color:#595959;
	border:1px solid #222;
	color:#c4c4c4;
	/*selection-background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#787878,stop:1#5a5a5a);*/
	/*selection-color:#000;*/
	padding:1px;
	border-radius: 2px;
}

QLineEdit:read-only, QTextEdit:read-only, QPlainTextEdit:read-only, QSpinBox:read-only, QDoubleSpinBox:read-only {
	background-color:#414859;
}


QComboBox
{
	border:1px solid #000;
	background-color: qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#343434,stop:1#1d1d1d) ;
	color:#dcdcdc;
	padding:0 18px 0 3px;
	margin-left: 3px;
}

QComboBox:editable
{
	background-color: qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#343434,stop:1#1d1d1d) ;
	selection-background-color:#282828;
	selection-color:#dcdcdc;
}

QComboBox::drop-down
{
	subcontrol-origin:padding;
	subcontrol-position:top right;
	width:15px;
	border:0;
}

QComboBox::down-arrow
{
	image:url(theme:dropdown_arrow.png);
}

QGroupBox
{
	color:#c8c8c8;
	background-color:#3b3b3b;
	border:1px solid #222;
	margin-top:1.1em;
	margin-bottom:1em;
	padding: 3px;
}

QGroupBox::title
{
	border:0 solid red;
	font-weight:700;
	subcontrol-origin:margin;
	top:0;
}


QToolTip
{
	background-color: #648ccb;
	color: #dfdfdf;
}

QWizard QListView
{
	background-color:#3c3c3c;
	color:#dcdcdc;
}

QWizardPage .QFrame
{
	background-color:transparent;
	border:1px solid #000;
}

SearchLineEdit
{
	background-color:transparent;
	border:1px solid #000;
	padding:0;
}

SearchLineEdit > QLineEdit
{
	background-color:#5a5a5a;
}

:disabled
{
	color:#818181;
}

QMainWindow,QScrollBar,QTableView
{
	background-color:#444;
}

QMainWindow::separator
{
    background-color: #222;
    width:3px;
    height:3px;
}

QMainWindow::separator:hover
{
    background-color: #383838;
    color: white;
    padding-left: 4px;
    border: 1px solid #4b4b4b;
    spacing: 3px; /* spacing between items in the tool bar */
}


QPushButton:pressed, QToolButton::pressed
{
	/*background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#787878,stop:1#5a5a5a);*/
	background-color: #444;
	color: #999;
}

QHeaderView::section:hover,QHeaderView::section:pressed
{
	background-color: #555;
}

QComboBox:!editable,QComboBox::drop-down:editable,QComboBox:!editable:on,QComboBox::drop-down:editable:on
{
	background-color:#333333;
}

QTreeView:active,QTreeView:!active
{
	selection-background-color:#466dc6;
}

QTreeView::item:selected:!active,QTreeView::item:selected:active,QListView::item:selected:!active,QListView::item:selected:active,QTabBar::tab:!selected:hover
{
	background-color:#466dc6;
}

QTreeView::item:hover,QListView::item:hover
{
	background-color:#58628b;
}

QCheckBox::indicator{
	margin-left: 2px;	
	margin-top: 2px;	
}



QTreeView::indicator:unchecked,QCheckBox::indicator:unchecked
{
	image:url(theme:checkbox_unchecked.png);
}

QTreeView::indicator:unchecked:disabled,QCheckBox::indicator:unchecked:disabled
{
	image:url(theme:checkbox_unchecked_disabled.png);
}

QTreeView::indicator:checked,QCheckBox::indicator:checked
{
	image:url(theme:checkbox_checked.png);
}

QTreeView::indicator:checked:disabled,QCheckBox::indicator:checked:disabled
{
	image:url(theme:checkbox_checked_disabled.png);
}

QListView::item:alternate,QLabel,QCheckBox,QRadioButton
{
	color:#dcdcdc;
}

QMenuBar,QToolBar:horizontal,QToolBar:vertical
{
	background-color:#282828;
}

QMenuBar::item:selected,QDialog WScrollArea
{
	background-color:#464646;
	color:#dcdcdc;
}

QToolButton::checked,QToolButton::menu-button:hover
{
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#b4b4b4,stop:1#969696);
}

#ObjectHeader {
	background-color:qlineargradient(x1:0,y1:0,x2:0,y2:1,stop:0#444,stop:1#333);
}

#ObjectInnerContainer{
	background-color:#444;
}

